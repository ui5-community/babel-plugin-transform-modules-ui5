{
  "name": "babel-plugin-transform-modules-ui5",
  "version": "7.0.5",
  "description": "An unofficial babel plugin for SAP UI5.",
  "main": "dist/index.js",
  "repository": "https://github.com/r-murphy/babel-plugin-ui5/tree/master/packages/plugin",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "build:watch": "babel src -d dist --watch",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "test": "jest __test__ --coverage=no",
    "test:watch": "npm test -- --watch",
    "prepare": "npm test && npm run build",
    "format": "prettier --write 'src/**/*.js' '__test__/*.js'"
  },
  "keywords": [
    "ui5",
    "sap",
    "sapui5",
    "openui5",
    "babel",
    "babel7",
    "babeljs",
    "babel-plugin",
    "babelplugin",
    "es6",
    "module",
    "import",
    "export",
    "typescript",
    "ts"
  ],
  "author": "Ryan Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-murphy/babel-plugin-transform-modules-ui5/issues"
  },
  "homepage": "https://github.com/r-murphy/babel-plugin-transform-modules-ui5#readme",
  "jest": {
    "collectCoverage": true
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "array-flatten": "^3.0.0",
    "doctrine": "^3.0.0",
    "ignore-case": "^0.1.0",
    "object-assign-defined": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-syntax-decorators": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.5",
    "core-js": "^3.18.2",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  }
}
